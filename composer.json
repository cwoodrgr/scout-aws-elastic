{
    "name": "ruger/scout-aws-elastic",
    "type": "library",
    "description": "Laravel Scout Elasticsearch driver with AWS support",
    "keywords": [
        "laravel",
        "scout",
        "elasticsearch",
        "search",
        "aws"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Wood",
            "email": "cwood@ruger.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^5.7",
        "laravel/scout": "^7.1",
        "aws/aws-sdk-php": "^3.97",
        "babenkoivan/scout-elasticsearch-driver": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Ruger\\ScoutAwsElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ruger\\ScoutAwsElastic\\ScoutAwsElasticServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
